{
    "title": "Force Paste",
    "rules": [{
        "description": "Force Paste",
        "manipulators": [
            {
                "description": "Force Paste",
                "type": "basic",
                "from": {
                    "key_code": "v",
                    "modifiers": {
                        "mandatory": [
                            "left_command",
                            "left_shift"
                        ]
                    }
                },
                "to": [
                    {
                        "shell_command": "/opt/homebrew/bin/python3.11 ~/.config/karabiner/assets/scripts/force_paste.py"
                    }
                ]
            }
        ]
    }]
}
